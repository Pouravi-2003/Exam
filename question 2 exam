#include<bits/stdc++.h>
#include<string.h>
using namespace std;
int main(){
    string str1;
	cin>>str1;
	int ans;

	//int i=0;
	for(int i=0;i<str1.length();i++){
int index = 0;
	for(int j=i+1;j<str1.length();j++){
		if(str1[i]==str1[j]){
			index = j;
			break;
			}
		
	}
	if(index==0) {ans=i;break;}
	}
	cout<<ans<<endl;

}
